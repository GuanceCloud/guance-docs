{
	"ignorePatterns": [
		{
			"pattern": "github.com",
			"why": "maybe GFWed"
		},
		{
			"pattern": "en.wikipedia.org",
			"why": "maybe GFWed"
		},
		{
			"pattern": "console.amazonaws.cn",
			"why": "expected"
		},
		{
			"pattern": "console.aws.amazon.com",
			"why": "expected"
		},
		{
			"pattern": "http.*",
			"why": "disable-all-http-checking"
		}
	],
	"timeout": "5s",
	"retryOn429": true,
	"retryCount": 1,
	"fallbackRetryDelay": "30s",
	"aliveStatusCodes": [200, 206, 0]
}
